<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.rongcloud.sealcall">

    <application
        android:name="cn.rongcloud.sealcall.SealCallApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.rongcloud.sealcall.MainActivity"></activity>
        <activity android:name="cn.rongcloud.sealcall.LoginActivity"></activity>
        <activity android:name="cn.rongcloud.sealcall.SelectUserActivity"></activity>
        <activity android:name="cn.rongcloud.sealcall.TestAttributionActivity"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>